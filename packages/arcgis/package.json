{
  "name": "@terraformer/arcgis",
  "description": "Convert ArcGIS JSON geometries to GeoJSON geometries and vica-versa.",
  "version": "2.0.0",
  "author": "Patrick Arlt <patrick.arlt@gmail.com>",
  "bugs": {
    "url": "https://github.com/terraformer-js/terraformer/issues"
  },
  "contributors": [
    "John Gravois <john@bikebbq.com>"
  ],
  "dependencies": {
    "@terraformer/common": "2.0.0"
  },
  "files": [
    "dist/**"
  ],
  "homepage": "https://github.com/terraformer-js/terraformer",
  "keywords": [
    "arcgis",
    "convert",
    "geo",
    "geojson",
    "geometry"
  ],
  "license": "MIT",
  "main": "dist/t-arcgis.umd.js",
  "unpkg": "dist/t-arcgis.umd.js",
  "jsdelivr": "dist/t-arcgis.umd.js",
  "module": "dist/t-arcgis.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/terraformer-js/terraformer"
  },
  "scripts": {
    "build": "rollup -c ../../rollup.umd.config.js && rollup -c ../../rollup.esm.config.js",
    "doc": "jsdoc2md --files src/index.js --template README.hbs > README.md",
    "prepare": "npm run build && npm run doc"
  }
}
