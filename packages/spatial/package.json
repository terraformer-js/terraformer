{
  "name": "@terraformer/spatial",
  "description": "Spatial predicates for GeoJSON.",
  "version": "2.0.0",
  "author": "Patrick Arlt <patrick.arlt@gmail.com>",
  "bugs": {
    "url": "https://github.com/terraformer-js/terraformer/issues"
  },
  "contributors": [
    "Patrick Arlt <parlt@esri.com> (http://patrickarlt.com)",
    "Jerry Sievert <code@legitimatesounding.com> (http://legitimatesounding.com)",
